DELETE PLACEHOLDER HASH_PAIR_KEY -
  /LANGUAGE="perl5" -
DEFINE PLACEHOLDER HASH_PAIR_KEY -
  /LANGUAGE="perl5" -
  /NOAUTO_SUBSTITUTE -
  /TYPE=TERMINAL -
"hash key"
