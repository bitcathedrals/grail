DELETE PLACEHOLDER HASH_PAIR -
  /LANGUAGE="perl5" -
DEFINE PLACEHOLDER HASH_PAIR -
  /LANGUAGE="perl5" -
  /NOAUTO_SUBSTITUTE -
  /SEPARATOR="," -
  /DUPLICATE="VERTICAL" -
  /DESCRIPTION="a key value pair" -
  /TYPE=NONTERMINAL -
"{hash_pair_key} => {hash_pair_value}"
