DELETE PLACEHOLDER HASH_IDENT -
  /LANGUAGE="perl5" -
DEFINE PLACEHOLDER HASH_IDENT -
  /LANGUAGE="perl5" -
  /NOAUTO_SUBSTITUTE -
  /TYPE=TERMINAL -
