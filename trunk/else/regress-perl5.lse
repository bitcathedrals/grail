  /LANGUAGE="perl5" -
DEFINE TOKEN hash -
  /LANGUAGE="perl5" -
  /NOAUTO_SUBSTITUTE -
  /DESCRIPTION="naming function arguements" -
  /TYPE=NONTERMINAL -
"my %{hash_ident} = ("
"  {hash_pair}..."
");"
